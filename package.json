{
  "name": "powercord",
  "version": "1.0.0",
  "description": "A lightweight @discordapp client mod focused on simplicity and performance",
  "main": "index.js",
  "scripts": {
    "_env:stable": "cross-env DISCORDTYPE=stable",
    "_env:ptb": "cross-env DISCORDTYPE=ptb",
    "_env:canary": "cross-env DISCORDTYPE=canary",
    "plug": "npm run _env:stable -- node injectors/index.js inject",
    "plug:ptb": "npm run _env:ptb -- node injectors/index.js inject",
    "plug:canary": "npm run _env:canary -- node injectors/index.js inject",
    "unplug": "npm run _env:stable -- node injectors/index.js uninject",
    "unplug:ptb": "npm run _env:ptb -- node injectors/index.js uninject",
    "unplug:canary": "npm run _env:canary -- node injectors/index.js uninject",
    "inject": "npm run _env:canary -- node injectors/index.js inject",
    "uninject": "npm run _env:canary -- node injectors/index.js uninject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/powercord-org/powercord.git"
  },
  "keywords": [],
  "author": "Aetheryx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/powercord-org/powercord/issues"
  },
  "homepage": "https://github.com/powercord-org/powercord#readme",
  "dependencies": {
    "buble": "0.19.6",
    "less": "^3.9.0",
    "node-watch": "^0.6.1",
    "sass": "^1.18.0",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4"
  }
}
